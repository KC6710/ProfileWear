<style>

@page {
  margin: 0;
}

body {font-family: Helvetica, sans-serif;font-size:13px;}
.container{max-width: 680px; margin:0 auto;}
.logotype{background:#000;color:#fff;width:75px;height:75px;  line-height: 75px; text-align: center; font-size:11px;}
.column-title{background:#f5f5f5;text-transform:uppercase;padding:15px 5px 15px 15px;font-size:11px}
.column-detail{border-top:1px solid #f5f5f5;border-bottom:1px solid #f5f5f5;}
.column-header{background:#f5f5f5;text-transform:uppercase;padding:15px;font-size:11px;}
.row{padding:7px 14px;border-bottom:1px dashed #f5f5f5;}
.alert{background: #ffd9e8;padding:20px;margin:20px 0;line-height:22px;color:#333}
.socialmedia{background:#f5f5f5;padding:20px; display:inline-block}
table{border-collapse: collapse;}

.totals span {width:100px;display:inline-block;padding-right:10px}
.totals div {background:#f5f5f5; padding:10px; margin-top:5px;}
</style>

<div style="width:100%;">
  
 <table width="100%" style="background:#F9D8E8;">
    <tr>
      <td style="padding:20px;">
        <br>      
        <div style="font-size:28px;font-weight:bold; padding-bottom:10px;float:left;">Offert</div><br>
              <br><br>
              Offert nr: {{quotation_id}}
      </td>
      <td style="padding:20px;">
              <div style="float:right; padding-bottom:10px"> <img src="data:image/svg+xml;base64," width="80px;" style="padding-right:10px;padding-bottom:10px;"><br></div>        
      </td>      
    </tr>
  </table><br>  

  <table width="100%" style="font-size:14px;padding:20px;">
    <tr>
      <td>{{quotation_other_detail.vatnr}} / {{firstname}} {{lastname}}</td>
      <td align="right">{{date_added}}</td>
    </tr>
  </table><br>
  
  <table width="100%" style="padding:20px;">
    <tr>
      <td width="50%">
        <table>
          <tr>
            <td style="vertical-align: text-top;">
             <div style="background: #ffd9e8 url(data:image/png;base64,) no-repeat;width: 50px;height: 50px;margin-right: 10px;background-position: center;background-size: 25px;"></div>   
            </td>
            <td style="line-height:20px;">
              <strong>Betalning</strong><br>
              {% if payment_firstname %}
              {{payment_firstname}} {{payment_lastname}}<br>
              {{payment_address_1}}<br>
              {{payment_address_2}} <br>
              {{payment_country}}
              {% else %}
              <em>Inga uppgifter inlagda.</em>
              {% endif %}
            </td>
          </tr>
        </table>
      </td>
      <td width="50%">
        <table>
          <tr>
            <td style="vertical-align: text-top;">
             <div style="background: #ffd9e8 url(data:image/png;base64,);width: 50px;height: 50px;margin-right: 10px;background-position: center;background-size: 42px;"></div>  
            </td>
            <td style="line-height:20px;">
              <strong>Leverans</strong><br>
              Leveransmetod: {{shipping_method}}<br>
              {% if shipping_firstname %}
              {{shipping_firstname}} {{shipping_lastname}}<br>
              {{shipping_address_1}}<br>
              {{shipping_address_2}} {{shipping_city}}<br>
              {{shipping_country}}
              {% endif %}              
            </td>
          </tr>
        </table>
      </td>
    </tr>
  </table><br>

   <table width="100%" style="border-collapse: collapse;padding:20px;">
     <tr>
       <td width="40%" class="column-header">Artikel</td>
       <td width="20%" class="column-header"></td>
       <td width="10%" class="column-header">A-Pris</td>
       <td width="5%" class="column-header">%</td>
       <td width="15%" class="column-header">Pris</td>
       <td width="20%" class="column-header">Totalt</td>
     </tr>
    {% if order_products %}
	{% for order_product in order_products %}
     <tr>
       <td class="row"><span style="color:#777;font-size:11px;">#{{ order_product.model }}</span><br>{{order_product.name}}</td>
       <td class="row">{{order_product.quantity}} x
       {% for options in order_product.option %}
       {# {% if count %}  #}
       {% for product_option_value in options.product_option_value %}
	   {% if product_option_value.selected =='selected' %}
            {{product_option_value.name}}              
            {% if not loop.last %}
                 |
              {% endif %}
       {% endif %}
	   {% endfor %}
       {# {% endif %} #}
       {% endfor %}</td>
       <td class="row">{{order_product.org_price}} kr</td>
       <td class="row">
       {% if order_product.price_discount_percentage > 0 %}
          {{order_product.price_discount_percentage}}%
        {% endif %}
       </td>
       <td class="row">{{order_product.price}} kr</td>
       <td class="row">{{order_product.total}} kr</td>
     </tr>
     {% endfor %}
     {% endif %}  
  </table>
  <!--
  <div style="background:#f5f5f5;padding:10px;text-align:right;">Total articles cost: 4500.00 kr</div>
  -->
    
   {# <table width="100%" style="border-collapse: collapse;">
     <tr>
       <td width="40%" class="row"><span style="color:#777;font-size:11px;">#64000L</span><br>Softstyle Lady Fit</td>
       <td width="20%" class="row">4 x Black | Large</td>
       <td width="10%" class="row">100 SEK</td>
       <td width="5%" class="row">25%</td>
       <td width="15%" class="row">75 SEK</td>
       <td width="20%" class="row">300 SEK</td>
     </tr>
     <tr>
       <td class="row"><span style="color:#777;font-size:11px;">#64000L</span><br>Softstyle Lady Fit</td>
       <td class="row">4 x Black | Large</td>
       <td class="row">100 SEK</td>
       <td class="row">25%</td>
       <td class="row">75 SEK</td>
       <td class="row">300 SEK</td>
     </tr>   
  </table>
  <div style="background:#f5f5f5;padding:10px;text-align:right;">Total print cost: 4500.00 kr</div>
  <br><br> #}

  <table width="100%" style="border-top:dashed 1px #eee;border-collapse: collapse; padding:20px;">
  <tr>
    <td style="border-right:10px solid #fff;" width="70%"></td>
    <td class="totals" style="line-height:20px;text-align:right;">
      <br>
      {% set sub_total = 0 %}
      {% if quotation_totals %}
		{% for total in quotation_totals %}
        {% if total.title == 'Sub-Total' %}
        {% set sub_total = total.text %}
        {% endif %}
        {% if total.title == 'Shipping' %}
          Frakt: <span>{{total.text}} kr</span><br>
          {% elseif total.title == 'Tax' %}
          Moms 25%: <span>{{total.text}} kr</span><br>
          {% else %}
          {{total.title}}: <span>{{total.text}} kr</span><br>
        {% endif %}
        {% endfor %}
      {% endif %}
      <div>Totalt exkl. moms:<br><div style="font-size:18px;padding:0;font-weight:bold;">{{ sub_total }} kr</div></div>
    </td>
  </tr>
</table>
         
  
</div><!--wrapper-->